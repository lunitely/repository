pkgname=lunitely-keyring
pkgver=1
pkgrel=1
pkgdesc="Lunitely PGP Keyring"
arch=('any')
url="https://lunitely.hackrland.dev"
license=('MIT, GPL')
install=$pkgname.install
source=('lunitely.gpg')
sha256sums=('6bd94befa56745abf9125dfa239a0fa14bc0269fe010ec50777b49476296c5b2')

package() {
  install -Dm644 lunitely.gpg "$pkgdir/usr/share/pacman/keyrings/lunitely.gpg"
}
